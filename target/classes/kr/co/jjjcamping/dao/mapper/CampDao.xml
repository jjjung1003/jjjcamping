<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybats-3-mapper.dtd">

<mapper namespace="kr.co.jjjcamping.dao.CampDao">

	
	
	<select id="camp_list" resultType="kr.co.jjjcamping.dto.CampDto">
		select * from camp
	</select>

	<select id="camp_content" resultType="kr.co.jjjcamping.dto.CampDto">
		select * from camp
		where id=#{param}
	</select>










</mapper>