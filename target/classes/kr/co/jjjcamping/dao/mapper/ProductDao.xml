<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybats-3-mapper.dtd">

<mapper namespace="kr.co.jjjcamping.dao.ProductDao">

	<select id="pro_list" resultType="kr.co.jjjcamping.dto.ProductDto">
		select * from product
		where code like concat(#{param1},'%')
		limit ${param2}, 10
	</select>
	
	<select id="get_category" resultType="kr.co.jjjcamping.dto.ProductDto">
		select * from category
		where code like concat(#{param1},'%')
	</select>
	








</mapper>